
pipeline{
    agent any    
    stages{
        stage('git'){
           steps{
              git 'https://github.com/23Aprujha/sample-web-application.git'     
           }
	}	
        stages{
             stage('build'){
                steps{
                  mvn clean install 
                }
	}			   
		
     }
		    
		    
                      
                    
	
